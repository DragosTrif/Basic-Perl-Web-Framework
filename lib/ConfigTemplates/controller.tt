use Moo;

use lib "[% superclass_path %]";
extends "[% superclass %]";

use lib "[% modell_path %]";
use [% modell %];

[% FOREACH a IN attributes %]
has [% a.name %] => (is => "ro", default => sub {[% a.default %]});
[% END %]

[% FOREACH m IN methods %]
sub [% m.name %] {
[% m.body %]
}
[% END %]

__PACKAGE__->meta->make_immutable;

1;
Class